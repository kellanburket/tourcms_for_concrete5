<?xml version="1.0"?>
<schema version="0.3">
	<table name="atTourInfoOptions">
		<field name="ID" type="I" size="10">
			<KEY/>
			<UNSIGNED/>
			<AUTOINCREMENT/>
		</field>
		<field name="akID" type="I" size="10">
			<UNSIGNED/>
		</field>
		<field name="tour_id" type="I" size="10">
			<UNSIGNED/>
		</field>
		<field name="tour_name" type="C" size="255">
		</field>
		<field name="displayOrder" type="I" size="10">
			<UNSIGNED/>
		</field>
		<field name="isEndUserAdded" type="I1" size="1">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
	</table>

	<table name="atTourInfoOptionsSelected">
		<field name="avID" type="I" size="10">
			<UNSIGNED/>
		</field>
		<field name="ctID" type="I" size="10">
			<KEY/>
			<UNSIGNED/>
		</field>
		<field name="atTourInfoOptionID" type="I" size="10">
			<KEY/>
			<UNSIGNED/>
		</field>
		
	</table>
</schema>